    "react-apollo": "^2.1.9",
    "react-loadable": "^5.4.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-29.0.0.tar.gz",
    "react-navigation": "^2.9.3",
    "react-redux": "^5.0.7",
